{
  "name": "grpc-discount-api",
  "version": "1.0.0",
  "description": "gRPC Discount Api",
  "main": "dist/server.js",
  "author": "Henrique martins de Souza",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/henriquemsouza/gRPC-Discount-Api"
  },
  "scripts": {
    "lint": "eslint src/  --ext .ts ",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build:proto": "node bin/proto",
    "start": "node dist/server",
    "test": "NODE_ENV=test mocha 'test/**/*.spec.ts'"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.7",
    "@types/sinon": "^9.0.4",
    "@types/sinon-chai": "^3.2.5",
    "chai-as-promised": "^7.1.1",
    "date-fns": "^2.24.0",
    "dotenv": "^10.0.0",
    "google-protobuf": "^3.17.3",
    "inversify": "^5.1.1",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.3",
    "@types/google-protobuf": "^3.15.5",
    "@types/lodash": "^4.14.175",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "dtslint": "^4.1.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-sonarjs": "^0.10.0",
    "grpc-tools": "^1.11.2",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "mocha": "^9.1.2",
    "protobufjs": "^6.11.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "ts-node": "^10.2.1",
    "tscpaths": "^0.0.9",
    "typescript": "^4.4.2"
  },
  "mocha": {
    "file": "test/globals/hooks.ts",
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  },
  "nyc": {
    "statements": 70,
    "branches": 70,
    "functions": 70,
    "lines": 70,
    "include": [
      "src"
    ],
    "exclude": [
      "docs/",
      "disc/",
      "node_modules/",
      "**/migrations/*"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "report-dir": "./coverage"
  }
}
